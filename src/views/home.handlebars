<div class="container"></div>
    <h1 class="">PRODUCTS</h1>

    <div class="">
        {{#each listadeproductos}}
        <div class="">
            <div class="card">
                <div class="card">
                    <i class=""></i> Code: {{this.code}}
                </div>
                <div class="card-body">
                    <h5 class="card-title">{{this.title}}</h5>
                    <ul class="list">
                        <li><i class=""></i> ID: {{this.id}}</li>
                        <li><i class=""></i> Description: {{this.description}}</li>
                        <li><i class=""></i> Price: ${{this.price}}</li>
                        <li><i class=""></i> Category: {{this.category}}</li>
                        <li><i class=""></i> Status: {{this.status}}</li>
                        <li><i class=""></i> Stock: {{this.stock}}</li>
                        <li>
                            <i class=""></i> Image: <img src={{this.thumbnail}} alt="{{this.title}}"
                                class="">
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>